<!DOCTYPE html>
<html>

<head>
  <title><%= title %></title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <script src="https://code.jquery.com/jquery-1.12.4.min.js"></script>
</head>

<body>
  <h1 id="h1_main"><%= title %></h1>
  <h1 class="text_green">other</h1>
  <p class="text_green">Welcome to <%= title %></p>
  <img id="img_logo" src="images/vslogo.png" alt="logo" />
  <ul id="list">
    <li>사과</li>
    <li>배</li>
  </ul>
  <button type="button" id="btn_01">수정</button>
</body>
<script>
  var count = 0;
  count++;
  $("#btn_01").click(function () {

    $("#list").append("<li>수박</li>");
    $("#h1_main").html("<strong>환영해요</strong>");
    $("#h1_main").toggleClass("text_red");
    $(".text_green").toggleClass("text_red");
    // $("img_logo").attr("src","google_logo.jpg");
    $("#img_logo").hide();

    if (count % 2 == 0)
      $("#img_logo").hide();
    else
      $("#img_logo").show();
  });
